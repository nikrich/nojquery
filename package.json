{
  "name": "nojquery",
  "version": "1.0.0",
  "description": "Library Description",
  "main": "lib/index.js",
  "scripts": {
    "build": "babel src --out-dir lib",
    "build:umd": "webpack src/index.js dist/nojquery.js",
    "build:umd:min": "cross-env NODE_ENV=production webpack src/index.js dist/nojquery.min.js",
    "clean": "rimraf lib dist coverage",
    "lint": "eslint src test examples",
    "test": "mocha --compilers js:babel-register --recursive",
    "test:cov": "babel-node ./node_modules/isparta/bin/isparta cover ./node_modules/mocha/bin/_mocha -- --recursive",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls",
    "test:watch": "npm test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nikrich/nojquery.git"
  },
  "keywords": [],
  "author": "Jannik Richter",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nikrich/nojquery/issues"
  },
  "homepage": "https://github.com/nikrich/nojquery",
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.4",
    "babel-eslint": "^6.0.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "chai": "^3.5.0",
    "coveralls": "^2.11.9",
    "cross-env": "^1.0.7",
    "eslint": "^2.5.3",
    "isparta": "^4.0.0",
    "mocha": "^2.4.5",
    "mocha-lcov-reporter": "^1.2.0",
    "rimraf": "^2.5.2",
    "webpack": "^1.12.14"
  }
}
